# Application port
PORT=3000

# Database (example for MongoDB)
MONGO_URI=mongodb://username:password@host:port/dbname

# JWT secret
JWT_SECRET=your_jwt_secret_here

# Third-party APIs
API_KEY=your_api_key_here

# Any other environment-specific config
#EXAMPLE_FLAG=true
